<view class="content">
  <view class="water" wx:for="{{waterList}}" wx:for-item="item" style="{{goodsItemStyles}}">
    <image src="{{item.img}}" class="img"></image>
    <text class="assess">{{item.assess}}</text>
    <view class="user">
      <view class="headImg" >
         <image src="{{item.headImg}}" mode='widthFix'></image>
      </view>
     
      <text class="userName">{{item.userName}}</text>
      <view class="right">
        <image class="zan" src="/images/点赞.png"></image>
        <text class="num">{{item.num}}</text>
      </view>
    </view>
  </view>
</view>
